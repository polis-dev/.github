{
    "id": "rusty",
    "version": "0.1.0",
    "name": "Rusty",
    "description": "A template for rusty things.",
    "documentationURL": "https://github.com/polis-dev/.github/devcontainer/tree/main/templates/src/rusty",
    "licenseURL": "https://github.com/polis-dev/.github/devcontainer/blob/main/templates/LICENSE",
    "options": {
        "imageVariant": {
            "type": "string",
            "description": "Debian version (use bullseye on local arm64/Apple Silicon):",
            "proposals": [
                "bullseye",
                "buster"
            ],
            "default": "bullseye"
        },
        "favorite": {
            "type": "string",
            "description": "Choose your favorite color.",
            "proposals": [
                "red",
                "gold",
                "green"
            ],
            "default": "red"
        }
    },
    "platforms": [
        "Any"
    ]
}
